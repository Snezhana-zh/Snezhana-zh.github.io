<h1>Все автомобили</h1>
<h3>@Model.currCategory</h3>
<div class="row mt-5 mb-2">
    @{
        foreach (Car car in Model.allCars) //явный тип данный Car
        {
            @Html.Partial("AllCars", car) //будет искать в папке Shared, первый параметр это имя шаблона, второй параметр это объект который передаем
        }
        }
</div>

